[user]
	name = Nick Barrett
	email = njbarrett7@gmail.com

[branch]
  autosetuprebase = always

[color]
  ui = auto

[color "branch"]
  current = yellow reverse
  local   = yellow
  remote  = green

[color "diff"]
  meta = yellow
  frag = magenta
  old  = red
  new  = green

[color "status"]
  added     = yellow
  changed   = green
  untracked = cyan

[diff]
  tool = Kaleidoscope

[diff "exif"]
  textconv = exiftool

[difftool]
  prompt = false

[merge]
  log = true

[push]
  default = current

[rerere]
  enabled = 1

[url "git@github.com:"]
  insteadOf     = "gh:"
  pushInsteadOf = "github:"
  pushInsteadOf = "git://github.com/"

[url "git://github.com/"]
  insteadOf = "github:"

[url "git@gist.github.com:"]
  insteadOf     = "gst:"
  pushInsteadOf = "gist:"
  pushInsteadOf = "git://gist.github.com/"

[url "git://gist.github.com/"]
  insteadOf = "gist:"

[url "git@heroku.com:"]
  insteadOf = "heroku:"
[core]
	excludesfile = /Users/nick/.gitignore_global
	editor = nano
	fileMode = false
	name = Nick Barrett
	email = njbarrett7@gmail.com
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path =
[mergetool "sourcetree"]
	cmd = /Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[commit]
	template = /Users/nick/.stCommitMsg
[credential]
	UseHttpPath = true
	helper = "!aws codecommit credential-helper "
